@model Food;


@{
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}





<h1></h1>

<h4></h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit" method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Fid" />
            <div class="form-group">
                <label asp-for="FoodType" class="control-label"></label>
                <input asp-for="FoodType" class="form-control"  type="hidden"  />

            </div>
            <div class="form-group">
                <label asp-for="FoodName" class="control-label"></label>
                <input asp-for="FoodName" class="form-control"  type="hidden"  />
               
            </div>
            <div class="form-group">
                <label asp-for="Time" class="control-label"></label>
                <input asp-for="Time" class="form-control" type="hidden" />

            </div>
            <div class="form-group">
                <label asp-for="Suggestion" class="control-label"></label>
                <input asp-for="Suggestion" class="form-control" />
                <span asp-validation-for="Suggestion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Status" class="control-label"></label>
                <input asp-for="Status" class="form-control"  type="hidden"  />

            </div>





            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



