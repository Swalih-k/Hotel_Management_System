@model Review;


@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}





<h1>Replay</h1>

<h4>Review</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="ReviewEdit" method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Reviewid" />
            <div class="form-group">
                <label asp-for="Reviews" class="control-label"></label>
                <input asp-for="Reviews" class="form-control" />
              
            </div>
            <div class="form-group">
                <label asp-for="Replay" class="control-label"></label>
                <input asp-for="Replay" class="form-control" />
                <span asp-validation-for="Replay" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Hid" class="control-label"></label>
                <input asp-for="Hid" class="form-control" />
              
            </div>
          




            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



